#/bin/bash

thrift --gen py -out ../pygen types.thrift && thriftpyi . --output ../pygen/types --strict-optional
mv ../pygen/types/types.pyi ../pygen/types/ttypes.pyi
echo 'from . import ttypes' > ../pygen/types/__init__.pyi
thrift-typescript --outDir ../frontend/src/jsgen/ --sourceDir . types.thrift